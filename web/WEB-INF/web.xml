<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SetCookies</servlet-name>
        <servlet-class>SetCookies</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SFCookie</servlet-name>
        <servlet-class>SFCookie</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SetCookies</servlet-name>
        <url-pattern>/SetCookies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SFCookie</servlet-name>
        <url-pattern>/SFCookie</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
